{
  "name": "@zero-gravity/oracle",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --import tsx/esm --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "node --import tsx/esm scripts/apply-migration.ts",
    "test:listener": "node --import tsx/esm scripts/test-listener.ts",
    "test": "node --import tsx/esm --test test/signer.test.ts",
    "test:ai": "node --import tsx/esm scripts/test-ai-pipeline.ts",
    "verify-schema": "node --import tsx/esm scripts/verify-schema.ts",
    "seed-liquidity": "node --import tsx/esm scripts/seed-liquidity.ts",
    "check-swipes": "node --import tsx/esm scripts/check-swipes.ts > check_output.txt 2>&1",
    "test-broadcast": "node --import tsx/esm scripts/test-broadcast.ts",
    "test:e2e": "node --import tsx/esm scripts/e2e-swipe.ts"
  },
  "dependencies": {
    "@bitauth/libauth": "^3.0.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.43.0",
    "cashscript": "^0.12.0",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
